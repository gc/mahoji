    "eslint": "^7.32.0",
    "nodemon": "^2.0.16",
    "rimraf": "^3.0.2",
    "typescript": "^4.7.2",
    "discord.js": "^14.3.0"
  },
  "peerDependencies": {
    "discord.js": "^14.3.0"
  },
  "files": [
    "dist",
    "!dist/*.tsbuildinfo"
  ]
