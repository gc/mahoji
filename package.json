{
  "name": "mahoji",
  "version": "0.0.1",
  "main": "dist/index.js",
  "scripts": {
    "dev:build": "tsc -p src",
    "dev:lint": "eslint \"{src,tests}/**/*.ts\" --fix",
    "dev:watch": "tsc -w -p src",
    "test": "yarn test:build && yarn test:run",
    "test:build": "tsc -p tests",
    "test:run": "jest --bail --runInBand --coverage --verbose",
    "test:watch": "yarn test:wipefolder && nodemon --config nodemon.json",
    "test:wipefolder": "rimraf \"compiledtests/\""
  },
  "license": "MIT",
  "dependencies": {
    "@discordjs/rest": "^0.1.0-canary.0",
    "fastify": "^3.21.0",
    "form-data": "^4.0.0",
    "node-fetch": "^2.6.3"
  },
  "devDependencies": {
    "@oldschoolgg/eslint-config": "^1.2.4",
    "@types/jest": "^26.0.23",
    "@types/mitm": "^1.3.4",
    "@types/node": "^16.11.4",
    "@types/node-fetch": "^2.5.3",
    "discord-api-types": "0.25.0-next.fa54b9d.1637238791",
    "eslint": "^7.32.0",
    "jest": "^27.0.1",
    "mitm": "^1.7.2",
    "nodemon": "^2.0.12",
    "rimraf": "^3.0.2",
    "typescript": "^4.4.3"
  }
}
